<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chunjae.edumarket.per.FoodMapper">
    <!-- 학교 정보 갖고오기 -->
    <select id="getSchool" resultType="com.chunjae.edumarket.entity.School">
        SELECT * FROM school WHERE sc_name LIKE CONCAT('%',#{sc_name},'%') limit 1
    </select>
</mapper>